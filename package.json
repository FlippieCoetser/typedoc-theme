{
  "name": "@mxssfd/typedoc-theme",
  "version": "1.0.4",
  "main": "dist/index.js",
  "repository": "https://github.com/mengxinssfd/typedoc-theme.git",
  "author": "DYH <xinzon32177@163.com>",
  "license": "MIT",
  "keywords": [
    "typedoc-theme"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "LICENSE",
    "assets"
  ],
  "engines": {
    "node": ">= 14"
  },
  "devDependencies": {
    "@types/node": "^18.15.10",
    "prettier": "^2.8.7",
    "typedoc": "^0.23.28",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "typedoc": "^0.23.21"
  },
  "scripts": {
    "ts-check": "tsc --noEmit",
    "build": "tsc",
    "docs": "tsc && typedoc src --plugin ./dist/index.js --theme my-theme",
    "docs:only": "typedoc src --plugin ./dist/index.js --theme my-theme"
  }
}
