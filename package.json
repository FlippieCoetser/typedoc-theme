{
  "name": "@mxssfd/typedoc-theme",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "https://github.com/mengxinssfd/typedoc-theme.git",
  "author": "DYH <xinzon32177@163.com>",
  "license": "MIT",
  "keywords": [
    "typedoc-theme"
  ],
  "engines": {
    "node": ">= 14"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "typedoc": "^0.23.21"
  },
  "scripts": {
    "ts-check": "tsc --noEmit",
    "build": "tsc",
    "predocs": "tsc",
    "docs": "typedoc src --plugin ./dist/index.js --theme my-theme",
    "docs:only": "typedoc src --plugin ./dist/index.js --theme my-theme"
  }
}
